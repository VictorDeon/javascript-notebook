{
  "compilerOptions": {
    // Define a versão do javascript e inclui declarações de bibliotecas compativeis
    "target": "ES2022",
    // Especifica que tipo de codigo é gerado
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
