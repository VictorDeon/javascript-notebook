{
  "compilerOptions": {
    // Define a versão do javascript de saída
    "target": "ES5",
    // Especifica que tipo de codigo é gerado
    "module": "CommonJS",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": ["ES2015", "DOM"]
  }
}
